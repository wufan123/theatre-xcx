.ticket-bg{position: relative;z-index: 0;width: 100%;height: 500rpx;}
.ticket-body{position: relative;z-index: 1;margin: -300rpx 100rpx 0 100rpx;}

.ticket .info image.bg,.code image.bg{width: 100%;height: 100%;position: absolute;top: 0;left: 0;z-index: 1}
.ticket .info{padding: 30rpx 0rpx;position: relative;}
.ticket .info text{display: block;padding-left: 30rpx;z-index: 2;position: relative;}

.code{padding: 60rpx 0rpx;position: relative;}
.code text,.code .code-img{position: relative;z-index: 2;}
.code-img{width: 200rpx;height: 200rpx;margin-top: 20rpx;}

.cell-line{padding: 20rpx 0rpx;}
.cell-line .title{width: 100%;height: 93rpx;}
.cell-line .title image{width: 100%;height: 100%;}

.order{background: none;padding: 10rpx 0;}
.order .line{padding: 10rpx 0;}